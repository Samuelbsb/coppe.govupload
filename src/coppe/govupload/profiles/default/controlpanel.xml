<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="coppe.govupload"
    purge="False">

    <configlet
        title="Upload Settings"
        action_id="upload"
        appId="coppe.govupload"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@coppe-upload-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
